<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="main" Color="MediumPurple"/>

    <SolidColorBrush x:Key="icon_foreground" Color="#696969"/>

    <SolidColorBrush x:Key="border_brush" Color="LightGray"/>

    <SolidColorBrush x:Key="border_background" Color="#f5f5f5"/>

    <SolidColorBrush x:Key="badge_forground" Color="DarkGray"/>

    <SolidColorBrush x:Key="mouse_over" Color="#dcdcdc"/>

    <SolidColorBrush x:Key="selected" Color="#d3d3d3"/>

    <SolidColorBrush x:Key="default_background" Color="White"/>

    <SolidColorBrush x:Key="menu_background" Color="#4169E1"/>

</ResourceDictionary>